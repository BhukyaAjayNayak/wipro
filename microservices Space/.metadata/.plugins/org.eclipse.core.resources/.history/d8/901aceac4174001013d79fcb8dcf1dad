package com.example.order_service.model;

import java.time.LocalDateTime;

@Entity
@Table(name="orders")
public class Order {
	private Long id;

	 @Id
	    @GeneratedValue(strategy = GenerationType.IDENTITY)
	    private Long id;
	    
	    @JsonIgnore
	    @OneToOne(mappedBy = "order")
	    private Payment payment;
	    
	    private LocalDateTime orderDate;
 

}
